{
    "name": "jasob-namesbag-generate",
    "displayName": "jasob-namesbag-generate",
    "description": "generate jasob namesbag from folder",
    "version": "0.0.1",
    "publisher": "xiangqian",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.generateClassName",
        "onCommand:extension.generateClassAndFunction"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.generateClassName",
                "title": "jasobNamesbag: generateClassName"
            },
            {
                "command": "extension.generateClassAndFunction",
                "title": "jasobNamesbag: generateClassAndFunction"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.10.9",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    }
}
